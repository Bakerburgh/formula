{
  "name": "functionfoundry",
  "version": "0.12.4",
  "description": "Pure function JavaScript library",
  "main": "fn/_index.js",
  "scripts": {
    "test": "babel-tape-runner test/*.js | faucet",
    "docs": "groc src/*.js",
    "test:docs": "docco --output test_docs test/*.js",
    "clean": "rm -rf fn",
    "build:js": "gulp",
    "build": "npm run clean && npm run build:js && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebsiteHQ/functionfoundry.git"
  },
  "keywords": [
    "Spreadsheets",
    "JavaScript"
  ],
  "author": "Peter Moresi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WebsiteHQ/functionfoundry/issues"
  },
  "homepage": "https://github.com/WebsiteHQ/functionfoundry#readme",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-tape-runner": "^2.0.1",
    "docco": "^0.7.0",
    "eslint": "^1.8.0",
    "faucet": "0.0.1",
    "groc": "^0.8.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "rollup": "^0.26.2",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1"
  }
}
